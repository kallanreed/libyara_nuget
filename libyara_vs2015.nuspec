<?xml version="1.0"?>
<package>
  <metadata>
    <id>libyara_vs2015</id>
    <title>libyara for VS2015</title>
    <version>3.5.1</version>
    <authors>kylereed</authors>
    <owners>kylereed</owners>
    <licenseUrl>https://github.com/kallanreed/libyara_nuget/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/kallanreed/libyara_nuget</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>VirusTotal's libyara compiled with VS2015 toolchain from the 3.5 release. Provides libs and headers to be included in native projects that use libyara.</description>
    <releaseNotes>Built from released 3.5.1 source</releaseNotes>
    <copyright>Copyright 2016</copyright>
    <tags>virustotal libyara yara 3.5 2015</tags>
      <dependencies>
    <dependency id="yara-vs2015-binary-dependencies" version="0.0.1" />
  </dependencies>
  </metadata>
  <files>
    <file src="yara\*.h" target="lib\native\include\" />
    <file src="yara\libyara\*.h" target="lib\native\include\" />
    <file src="yara\libyara\include\*.h" target="lib\native\include\" />
    <file src="yara\libyara\include\yara\*.h" target="lib\native\include\yara\" />
    <file src="yara\windows\vs2015\libyara\Release\libyara32.lib"
          target="lib\native\lib\libyara32.lib" />
    <file src="yara\windows\vs2015\libyara\Release\libyara64.lib"
          target="lib\native\lib\libyara64.lib" />
    <file src="build\libyara_vs2015.targets" target="build\native\libyara_vs2015.targets" />
  </files>
</package>
